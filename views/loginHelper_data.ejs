<!doctype html>
<script type="text/javascript">
	$(document).ready(function(){
		$('#ourl').val(function(){
			var search = window.location.search.substring(1) ; 
			var arr = search.split('&');
			for(var i = 0 ; i < arr.length ; i ++){
				var s = arr[i].split('=');
				if(s[0] === 'ourl')
					return s[1]; 
			}
		});
	});
</script>
